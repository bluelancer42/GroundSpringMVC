<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>GroundSpringMVC</display-name>
	<welcome-file-list>
		<welcome-file>welcome.html</welcome-file>
		<welcome-file>welcome.htm</welcome-file>
		<welcome-file>WEB-INF/pages/welcome.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>ground</servlet-name>
		<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>ground</servlet-name>
		<url-pattern>/</url-pattern>
		<url-pattern>/welcome.jsp</url-pattern>
		<url-pattern>/index.jsp</url-pattern>
		<url-pattern>/welcome.html</url-pattern>
		<url-pattern>*.html</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>admin</servlet-name>
		<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>admin</servlet-name>
		<url-pattern>/admin.jsp</url-pattern>
		<url-pattern>/admin.html</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>faq</servlet-name>
		<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>faq</servlet-name>
		<url-pattern>/faq.jsp</url-pattern>
		<url-pattern>/faq.html</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>myaccount</servlet-name>
		<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>myaccount</servlet-name>
		<url-pattern>/myaccount.jsp</url-pattern>
		<url-pattern>/myaccount.html</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>subscribe</servlet-name>
		<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>subscribe</servlet-name>
		<url-pattern>/subscribe.jsp</url-pattern>
		<url-pattern>/subscribe.html</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>subscriptions</servlet-name>
		<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>subscriptions</servlet-name>
		<url-pattern>/subscriptions.jsp</url-pattern>
		<url-pattern>/subscriptions.html</url-pattern>
	</servlet-mapping>
</web-app>